
public class Variablen {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		// 8 primitive Datenypen in Java
		/*byte
		 * short
		 * long
		 * int
		 * long
		 * float
		 * boolean
		 * char
		 * 		
		*/
		// <Datenype> <Variabelname> = <Wert>;
		
		
		int varZahl1 = 3;
		long varZahl2 = 300;
		
		double varZahl3 = 322.23;
		float varZahl4 = 23.34f;
		
		boolean userKlick = true;
		boolean userKlicknot = false;
		
		char zeichen = "!";
		

	}

}
