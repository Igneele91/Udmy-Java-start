
public class Zeichenkette {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 8 primitive Datenypen in Java
				/*
				 * byte
				 * short
				 * long
				 * int
				 * long
				 * float
				 * boolean   (Ausgesprochen: bool)
				 * char      (Ausgesprochen: charakter = Zeichen)
				 * 		
				 */
				 // <Datenype> <Variabelnamen> = <Wert>;
		char varZeichen = '!';
		
		String varZeichenkette = "Ich bin eine Zeichenkette";
		System.out.println(varZeichenkette);
		
		
		String name = "Christian";
