
public class ZeichenBool {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 8 primitive Datentypen in Java
		/*
		 * byte
		 * short
		 * long
		 * int
		 * long
		 * float
		 * boolean   (Ausgesprochen: bool)
		 * char      (Ausgesprochen: charakter = Zeichen) 		
		 */
		
		// Komplexe Datetypen
		// String (Zeichenkette)
		
		// <Datenype> <Variabelnamen> = <Wert>;
		
		char varZeichen1 = '\u00A9';
		char varZeichen2 = '\u00AE';
		char varZeichen3 = '\u269D';
		
		/*
		// (Link für die Zeichen hier: https://unicode-table.com/de/269D/ )  
		*/
		
		System.out.println("Das Bild unterliegt dem " +  varZeichen1);
		System.out.println("Das Bild unterliegt dem " +  varZeichen2);
		System.out.println("Das Bild unterliegt dem " +  varZeichen3);
		

	}

}
